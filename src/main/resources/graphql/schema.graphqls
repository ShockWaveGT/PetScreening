type Breed{
    id: ID!
    name: String!
}

type Pet{
    id: ID!
    name: String!
    weight: Float!
    vaccinationStatus: Boolean!
    trainingLevel: Int!
    breedID: Breed!
}

type Mutation{
    addPet(name: String! ,weight: Float! ,breedId: Int! ,vaccinated: Boolean! ,trainingLevel: Int!): Pet!
}

type Query{
    weightLessThan(weight: Float!): [Pet]
}